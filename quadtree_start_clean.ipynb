    "const EPSILON = 1e-4\n",
    "\n",
    "function repulsive_gradient(u, v)\n",
    "    d = sqrt(u^2 + v^2 + EPSILON)^3\n",
    "    return (u/d, v/d)\n",
    "    end;\n"
   ]
