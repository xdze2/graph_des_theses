    "function count_cells(root)\n",
    "    count = 1\n",
    "    if ~isnothing(root.childrens)\n",
    "        for quadrant in root.childrens\n",
    "            count += count_cells(quadrant)\n",
    "        end\n",
    "    end\n",
    "\n",
    "    return count\n",
    "end\n"
   ]
