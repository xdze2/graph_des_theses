    "# brute force\n",
    "function brute_force_gradient!(G, points)\n",
    "    fill!(G, 0.f0)\n",
    "    for (i, (x1, y1)) in enumerate( eachcol(points) )\n",
    "        for (j, (x2, y2)) in enumerate( eachcol(points[:, i+1:end]) )\n",
    "            j += i\n",
    "            u = x2 - x1\n",
    "            v = y2 - y1\n",
    "            grad_x, grad_y = repulsive_gradient(u, v)\n",
    "            G[1, i] += grad_x\n",
    "            G[2, i] += grad_y\n",
    "            G[1, j] -= grad_x\n",
    "            G[2, j] -= grad_y\n",
    "            end;\n",
    "        end;\n",
    "    end;"
   ]
  },
