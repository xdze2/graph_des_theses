    "function build_the_tree(points)\n",
    "    root = Cell(0.0, 0.0, 1.0, 1.0, 0, nothing, nothing, nothing)\n",
    "    for (x, y) in eachcol(points)\n",
    "        insert!(root, x, y)\n",
    "        end;\n",
    "    return root\n",
    "    end;"
   ]
